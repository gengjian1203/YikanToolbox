{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/actions/*": ["./src/actions/*"],
      "@/api/*": ["./src/api/*"],
      "@/components/*": ["./src/components/*"],
      "@/constants/*": ["./src/constants/*"],
      "@/kits/*": ["./src/kits/*"],
      "@/manager/*": ["./src/manager/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/scss/*": ["./src/scss/*"],
      "@/services/*": ["./src/services/*"],
      "@/reducers/*": ["./src/reducers/*"],
      "@/store/*": ["./src/store/*"]
    },
    "jsx": "preserve",
    "jsxFactory": "Taro.createElement",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "global.d.ts"
    ],
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compileOnSave": false
}
